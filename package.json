{
  "name": "eslint-plugin-uship-omniscient",
  "version": "0.12.0",
  "description": "An eslint rule to lint for, and fix, usages of omniscient.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Richard Simpson <rsimpson@uship.com>",
  "main": "dist/index.js",
  "scripts": {
    "lint:types": "tsc --noEmit",
    "build": "run-s clean lint:** build:**",
    "clean": "rimraf ./dist/",
    "build:typescript": "tsc",
    "test": "mocha tests --require ts-node/register \"./tests/**/*.spec.ts\"",
    "test:debug": "mocha tests --require ts-node/register \"./tests/**/*.spec.ts\" --inspect-brk",
    "prepublish": "run-s build"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.5",
    "@types/node": "^10.12.12",
    "@types/prettier": "^1.15.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  },
  "engines": {
    "node": ">=8.x"
  },
  "license": "ISC",
  "peerDependencies": {
    "eslint": "~5.9.0"
  },
  "dependencies": {
    "prettier": "^1.15.3"
  }
}
